@model MediaPortal.Models.FileSystemModels


<div class="col-sm-6 col-md-3 context-menu-file" data-name="@Model.Id" data-file-name="@Model.Name">
        <div class="thumbnail file-block-item">
            <img src="@Model.BlobLink" alt="blobLink">
            <div class="caption">
                <span id="iconFile"></span>
                <span class="file-name ">@Model.Name</span>
            </div>
        </div>
</div>

